CREATE TABLE public.producto
(
    id_pro integer Primary key,
    nombre_pro character varying(100),
    precio_pro numeric,
    stock_pro integer,
    descripcion_pro character varying(150),
    imagen_pro character varying(300),
    tipo_pro character varying(50)
)


